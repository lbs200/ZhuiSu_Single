        // finder.BaseUrl = '/ckfinder/jigou/';
        // finder.baseDir = resolveUrl(finder.BaseUrl);
        finder.selectActionFunction = SetFileField; //设置文件被选中时的函数
        finder.selectActionData = inputId; //接收地址的input ID
        finder.Width = 600;
        finder.Height = 425;
        finder.popup();


    }
