@model LinqModel.AdministrationRegion

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutPageManage.cshtml";
}

 

<h3>确认删除？</h3>
<div>
    <h4>行政区划</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            名称：
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Province)
        </dd>

        <dt>
            上级：
        </dt>

       @{
           LinqModel.AdministrationRegion ar = new DAL.Region().GetModel(Convert.ToInt32(Model.City));
        <dd>
           @ar.Province
        </dd>
    }

        <dt>
            邮编：
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Postal_Code)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="删除" class="btn btn-default" /> |
            @Html.ActionLink("返回列表", "Index")
        </div>
    }
</div>
